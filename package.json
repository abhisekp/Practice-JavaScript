{
  "name": "practice-modern-javascript",
  "version": "3.0.0",
  "description": "A boilerplate to setup and practice Modern JavaScript",
  "main": "index.js",
  "engines": {
    "node": ">=6"
  },
  "dependencies": {
    "better-console": "^0.2.4"
  },
  "devDependencies": {
    "babel-cli": "^6.7.7",
    "babel-plugin-transform-async-to-generator": "^6.7.4",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-polyfill": "^6.7.4",
    "babel-preset-modern-node": "^2.0.0",
    "babel-preset-stage-0": "^6.5.0"
  },
  "scripts": {
    "watch": "npm run -s watch:dev",
    "watch:dev": "nodemon -q -x babel-node index.js",
    "watch:prod": "nodemon -w index.es6.js -q index.es6.js",
    "start": "npm run -s build:prod & npm run -s watch:prod",
    "build": "npm run -s build:prod",
    "build:prod": "babel -w -o index.es6.js index.js 1>/dev/null",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/abhisekp/Practice-Modern-JavaScript.git"
  },
  "keywords": [
    "practice",
    "modern",
    "javascript",
    "es6",
    "es7",
    "es2015",
    "es2016"
  ],
  "author": "Abhisek Pattnaik <abhisekp@engineer.com> (http://about.me/abhisekp)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/abhisekp/Practice-Modern-JavaScript/issues"
  },
  "homepage": "https://github.com/abhisekp/Practice-Modern-JavaScript#readme"
}
