{
  "name": "practice-modern-javascript",
  "version": "3.2.1",
  "description": "A boilerplate to setup and practice Modern JavaScript",
  "main": "index.js",
  "engines": {
    "node": ">=6"
  },
  "dependencies": {
    "async": "^2.1.1",
    "better-console": "^0.2.4",
    "cfonts": "0.0.13",
    "colors": "^1.1.2",
    "decimal.js": "^5.0.8",
    "dedent": "^0.6.0",
    "lodash": "^4.12.0",
    "npm-run-all": "^2.1.1",
    "numd": "^3.0.2",
    "ora": "^0.2.3",
    "ordinal-number-suffix": "^0.1.1",
    "rimraf": "^2.5.2",
    "source-map-support": "^0.4.0"
  },
  "devDependencies": {
    "ava": "^0.14.0",
    "babel-cli": "^6.11.4",
    "babel-eslint": "^6.1.2",
    "babel-plugin-syntax-flow": "^6.8.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-flow-comments": "^6.8.0",
    "babel-plugin-transform-flow-strip-types": "^6.8.0",
    "babel-plugin-transform-member-expression-literals": "^6.8.0",
    "babel-plugin-transform-merge-sibling-variables": "^6.8.0",
    "babel-plugin-transform-minify-booleans": "^6.8.0",
    "babel-plugin-transform-property-literals": "^6.8.0",
    "babel-plugin-transform-remove-debugger": "^6.8.0",
    "babel-plugin-transform-simplify-comparison-operators": "^6.8.0",
    "babel-plugin-transform-undefined-to-void": "^6.8.0",
    "babel-plugin-typecheck": "^3.9.0",
    "babel-polyfill": "^6.9.1",
    "babel-preset-modern-node": "^2.2.1",
    "babel-preset-stage-0": "^6.5.0",
    "eslint": "^2.13.1",
    "eslint-config-airbnb-base": "^3.0.1",
    "eslint-plugin-babel": "^3.3.0",
    "eslint-plugin-flow-vars": "^0.4.0",
    "eslint-plugin-flowtype": "^2.4.0",
    "eslint-plugin-import": "^1.12.0",
    "flow-bin": "^0.25.0"
  },
  "scripts": {
    "watch": "npm-run-all watch:dev",
    "watch:prod": "npm-run-all -p build:watch:prod run:watch:prod",
    "run:watch:prod": "nodemon -q -w build build",
    "watch:dev": "nodemon -q -w src -x babel-node src",
    "start": "node build || npm-run-all build:prod start",
    "start:watch": "npm-run-all watch:prod",
    "build": "npm-run-all build:prod",
    "build:prod": "npm-run-all clean:build && babel -q -D -s --minified -d build src",
    "build:watch:prod": "npm-run-all clean:build && babel -q -w -s --minified -D -d build src",
    "build:dev": "",
    "test": "echo \"Error: no test specified\" && exit 1",
    "postinstall": "npm-run-all build",
    "clean": "npm-run-all clean:build",
    "clean:build": "rimraf build/*",
    "clean:node_modules": "rimraf node_modules",
    "clean:all": "npm-run-all -p clean:build clean:node_modules",
    "flow": "flow; test $? -eq 0 -o $? -eq 2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/abhisekp/Practice-Modern-JavaScript.git"
  },
  "keywords": [
    "practice",
    "modern",
    "javascript",
    "es6",
    "es7",
    "es2015",
    "es2016"
  ],
  "author": "Abhisek Pattnaik <abhisekp@engineer.com> (http://about.me/abhisekp)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/abhisekp/Practice-Modern-JavaScript/issues"
  },
  "homepage": "https://github.com/abhisekp/Practice-Modern-JavaScript#readme"
}
